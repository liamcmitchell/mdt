{
  "name": "mdt",
  "version": "1.0.0",
  "description": "MDT",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "webpack-dev-server",
    "build": "rm -rf build && webpack --env.prod && cp public/* build/ && cp public/index.html build/404.html",
    "deploy": "gh-pages --dist build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "eslint"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamcmitchell/mdt.git"
  },
  "author": "Liam Mitchell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liamcmitchell/mdt/issues"
  },
  "homepage": "https://github.com/liamcmitchell/mdt#readme",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.14.2",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.1.0",
    "history": "^4.6.1",
    "json-loader": "^0.5.4",
    "jsonschema": "^1.1.0",
    "keycode": "^2.1.0",
    "lodash": "^4.13.1",
    "prop-types": "^15.5.8",
    "qs": "^6.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.0.0",
    "react-io": "^4.4.8",
    "recompose": "^0.26.0",
    "rxjs": "^5.5.0",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-io": "^4.2.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.2"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "1.11.1"
