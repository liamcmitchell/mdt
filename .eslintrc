{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "rules": {
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }],
    "react/prop-types": "off",
    "react/display-name": "off"
  }
}